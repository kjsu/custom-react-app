    "prettier": "2.4.1",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^12.1.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.53.0",
